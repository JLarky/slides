{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "deno.enable": true,
  "deno.unstable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "dprint.dprint",
  "[javascript]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[astro]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "file"
    }
  ]
}
